
    
<%- include('layout'); -%>

<div class="relative overflow-x-auto w-full h-full">
    <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
        <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
            <tr>
                <th scope="col" class="px-6 py-3">
                     street
                </th>
                <th scope="col" class="px-6 py-3">
                    Description
                </th>
                <th scope="col" class="px-6 py-3">
                    logo
                </th>
                
                <th scope="col" class="px-6 py-3">
                    email
                </th>
                <th scope="col" class="px-6 py-3">
                    phone
                </th>
                <th scope="col" class="px-6 py-3">
                    location
                </th>
                <th scope="col" class="px-6 py-3">
                    address
                </th>
                <th scope="col" class="px-6 py-3">
                    Actions
                </th>
            </tr>
        </thead>
        <tbody>
            <% websetting.forEach(websettings => { %>
                <tr class="bg-white border-b dark:bg-gray-800 dark:border-gray-700">
                    <td class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                        <%= websettings.street %>
                    </td>
                    <td class="px-6 py-4">
                        <%= websettings.description %>
                    </td>
                    <td class="px-6 py-4">
                        <img src="http://localhost:5000/images/<%= websettings.logo %>" alt="" class="w-32">
                    </td>
                    
                    <td class="px-6 py-4">
                        <%= websettings.email %>
                    </td>
                    <td class="px-6 py-4">
                        <%= websettings.phone %>
                    </td>
                    <td class="px-6 py-4">
                        <%= websettings.location %>
                    </td>
                    <td class="px-6 py-4">
                        <%= websettings.address %>
                    </td>
                   
                    <td class="px-6 py-4">
                        <form action="/web-setting/edit/<%= websettings.id %>" class="inline"  method="get">
                            <button type="submit">
                                <i class="fas fa-edit"></i>
                             </button>
                            </form>                 
                            <form action="/web-setting/delete/<%= websettings.id %>?_method=delete" class="inline" method="post">
                                <input type="hidden" name="_method" value="delete">
                                <button type="submit">
                                    <i class="fas fa-trash m-3"></i>
                                 </button>
                            </form>                     </td>
                    <!-- <td class="px-6 py-4">
                        <i class="fas fa-trash"></i>
                    </td> -->
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>

</body>
</html>
